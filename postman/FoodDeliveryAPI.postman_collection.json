{
  "info": {
    "name": "Food Delivery API",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Register User",
      "request": {
        "method": "POST",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "url": { "raw": "http://localhost:3000/api/register", "host": ["localhost"], "port": "3000", "path": ["api", "register"] },
        "body": {
          "mode": "raw",
          "raw": "{\n  \"username\": \"testuser\",\n  \"password\": \"123456\",\n  \"role\": \"customer\"\n}"
        }
      }
    },
    {
      "name": "Login",
      "request": {
        "method": "POST",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "url": { "raw": "http://localhost:3000/api/login", "host": ["localhost"], "port": "3000", "path": ["api", "login"] },
        "body": {
          "mode": "raw",
          "raw": "{\n  \"username\": \"testuser\",\n  \"password\": \"123456\"\n}"
        }
      }
    },
    {
      "name": "Place Order",
      "request": {
        "method": "POST",
        "header": [
          { "key": "Content-Type", "value": "application/json" },
          { "key": "Authorization", "value": "Bearer {{jwtToken}}" }
        ],
        "url": { "raw": "http://localhost:3000/api/orders", "host": ["localhost"], "port": "3000", "path": ["api", "orders"] },
        "body": {
          "mode": "raw",
          "raw": "{\n  \"items\": [\"menu-id-1\", \"menu-id-2\"]\n}"
        }
      }
    }
  ]
}
